    }
    upper
    {
        type            zeroGradient;
    }
    west
    {
        type            zeroGradient;
    }
    east
    {
        type            zeroGradient;
    }
    north
    {
        type            zeroGradient;
    }
    south
    {
        type            zeroGradient;
    }

}


